
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch all perf_analyzer's unit tests",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/perf_analyzer/src/perf-analyzer-build/perf_analyzer_unit_tests",
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb"
    },
    {
      "name": "Launch coroutine tests",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/perf_analyzer/src/perf-analyzer-build/perf_analyzer_unit_tests",
      "args": ["-tc=coro*"],
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb"
    }
  ]
}
